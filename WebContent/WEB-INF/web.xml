<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JspProject2.0</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>

  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>ProvinceList</servlet-name>
    <servlet-class>servlet.ProvinceListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProvinceList</servlet-name>
    <url-pattern>/province/list.do</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>CityList</servlet-name>
    <servlet-class>servlet.CityListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CityList</servlet-name>
    <url-pattern>/city/list.do</url-pattern>
  </servlet-mapping>

  
</web-app>